<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>beginor</Authors>
    <Description>Extended PostgreSQL driver for NHibernate, with support of json and array types.</Description>
    <PackageId>NHibernate.Extensions.NpgSql</PackageId>
    <PackageVersion>3.1.6</PackageVersion>
    <Owners>beginor</Owners>
    <PackageProjectUrl>https://github.com/beginor/nhibernate-extensions</PackageProjectUrl>
    <Title>NHibernate.Extensions.NpgSql</Title>
    <PackageReleaseNotes>Update references (.net core 3.1.6, nhibernate 5.3.0) .</PackageReleaseNotes>
    <PackageTags>nhibernate postgresql npgsql</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="NHibernate" Version="5.3.1" />
    <PackageReference Include="Npgsql" Version="4.1.4" />
  </ItemGroup>
</Project>
