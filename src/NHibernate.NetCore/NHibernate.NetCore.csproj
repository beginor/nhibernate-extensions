<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>beginor</Authors>
    <Description>Extensions for NHibernate when used in dotnet core.
For examples, visit https://github.com/beginor/nhibernate-extensions/blob/master/test/NHibernate.Extensions.UnitTest/BaseTest.cs</Description>
    <PackageId>NHibernate.NetCore</PackageId>
    <PackageVersion>3.1.6</PackageVersion>
    <Owners>beginor</Owners>
    <Title>NHibernate.NetCore</Title>
    <PackageProjectUrl>https://github.com/beginor/nhibernate-extensions</PackageProjectUrl>
    <PackageReleaseNotes>Update nhibernate to 5.3.2</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="3.1.6" />
    <PackageReference Include="NHibernate" Version="5.3.2" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="3.1.6" />
  </ItemGroup>

</Project>
