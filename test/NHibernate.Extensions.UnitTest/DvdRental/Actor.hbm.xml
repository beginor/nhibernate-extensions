<hibernate-mapping
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns="urn:nhibernate-mapping-2.2"
  namespace="NHibernate.Extensions.UnitTest.DvdRental"
  assembly="NHibernate.Extensions.UnitTest">


  <class name="Actor" table="actor" schema="public">
    <id name="ActorId" column="actor_id" type="int">
      <generator class="sequence">
        <param name="sequence">actor_actor_id_seq</param>
      </generator>
    </id>

    <property name="FirstName" column="first_name" length="45" />
    <property name="LastName" column="last_name" length="45" />
    <property name="LastUpdate" column="last_update" />

  </class>

</hibernate-mapping>
