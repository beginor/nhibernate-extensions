<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns="urn:nhibernate-mapping-2.2"
  namespace="NHibernate.Extensions.UnitTest.NpgSql.Data"
  assembly="NHibernate.Extensions.UnitTest"
>

  <class name="Author" table="authors" schema="public">
    <id name="AuthorId" column="authorid" type="int">
      <generator class="native" />
    </id>
    <property name="Name" column="name" type="string" />
    <bag name="Books" table="books" inverse="true">
      <key column="authorid" />
      <one-to-many class="Book" />
    </bag>
  </class>

</hibernate-mapping>
